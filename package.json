{
  "name": "glasseater",
  "repository": "https://github.com/gootools/glasseater",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "source": "src/index.ts",
  "exports": {
    "require": "./lib/index.cjs",
    "default": "./lib/index.modern.js"
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.module.js",
  "unpkg": "./lib/index.umd.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "rimraf lib && microbundle --no-compress --no-sourcemap",
    "test": "jest"
  },
  "license": "MIT",
  "dependencies": {
    "@solana/web3.js": "^1.3",
    "borsh": "^0.7.0",
    "bs58": "^4",
    "cross-fetch": "^3.1.4"
  },
  "devDependencies": {
    "@types/bs58": "^4.0.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.7",
    "esbuild": "^0.14.10",
    "esbuild-jest": "^0.5.0",
    "jest": "^27.4.5",
    "microbundle": "^0.14.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "esbuild-jest"
    }
  }
}
