{
  "name": "glasseater",
  "repository": "https://github.com/gootools/glasseater",
  "version": "0.2.4",
  "private": false,
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/mjs/index.d.ts",
  "source": "src/index.ts",
  "exports": {
    "import": "./dist/mjs/index.js",
    "require": "./dist/cjs/index.js"
  },
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json && tsc -p tsconfig.cjs.json && ./bin/after-build.sh",
    "test": "jest",
    "release": "release-it"
  },
  "license": "MIT",
  "peerDependencies": {
    "@solana/web3.js": "^1.3"
  },
  "devDependencies": {
    "@solana/web3.js": "^1.3",
    "@types/bs58": "^4.0.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.7",
    "esbuild": "^0.14.10",
    "esbuild-jest": "^0.5.0",
    "jest": "^27.4.5",
    "release-it": "^14.11.8",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "esbuild-jest"
    }
  }
}
